{{!-- If you want TinyMCE editors to output inline rolls when rendered, you need to pass the actor's roll data to
the rollData property. --}}


<div class="resource-field-text">
    <label for="system.clan.name" class="resource-label">{{localize "Clan"}}</label>
    <input type="text" name="system.clan.name" value="{{system.clan.name}}" data-dtype="text" />
    <br><br>
    <label for="system.clan.description" class="resource-label">{{localize "Clan Description"}}</label>
    <textarea data-dtype="text" placeholder="{{localize " Type the clan description here..."}}"
        name="system.clan.description" value="{{system.clan.description}}">{{system.clan.description}}</textarea>
</div>
<br>
<h1>
    {{localize "Description"}}
</h1>
{{editor system.biography target="system.biography" rollData=rollData button=true owner=owner editable=editable}}